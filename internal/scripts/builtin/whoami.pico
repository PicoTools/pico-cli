alias(
  "whoami",
  fn (aid, cmd, args, raw) {
    if args.len() != 0 {
      c_error("invalid number of arguments");
      return m_error(aid, "invalid number of arguments");
    }

    c_notify("tasked agent to whoami");
    m_notify(aid, "tasked agent to whoami");
    return a_whoami(aid);
  },
  "print current agent's process user",
  "Usage:\n  whoami",
  true
);